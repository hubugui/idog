{
	"name": "iDog",
	"version": "1.1",
	"description": "This extension can block Chrome internet access when your need.",
	"background_page": "background.html",
	"options_page": "popup.html",
	"icons": 
	{
		"16": "icon.jpg",
		"48": "icon.jpg",
		"128": "icon.jpg"
	},
	"browser_action":
	{
		"default_icon": "icon.jpg"
	},

	"permissions":
	[
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	]
}
